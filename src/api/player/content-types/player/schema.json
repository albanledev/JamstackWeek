{
  "kind": "collectionType",
  "collectionName": "players",
  "info": {
    "singularName": "player",
    "pluralName": "players",
    "displayName": "Player",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "ranking": {
      "type": "integer",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "competitions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::competition.competition",
      "inversedBy": "players"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    }
  }
}
